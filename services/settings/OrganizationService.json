{
    "name": "Zoho invoice organization api",
    "basePath": "https://invoice.zoho.com",
    "operations": {
        "abstract.basezohoorgapi": {
            "summary": "base calls to zoho api",
            "parameters": {
                "authtoken": {
                    "location": "query",
                    "description": "User to retrieve by ID",
                    "required": true
                }
            }
        },
        "getOrganizations": {
            "httpMethod": "GET",
            "extends": "abstract.basezohoorgapi",
            "uri": "/api/v3/organizations",
            "summary": "List organizations"
        },
        "getOrganization": {
            "httpMethod": "GET",
            "extends": "abstract.basezohoorgapi",
            "uri": "/api/v3/organizations/{id}",
            "summary": "Gets an auth token",
            "parameters": {
                "id": {
                    "location": "uri",
                    "description": "Organization ID",
                    "required": true
                }
            }
        },
        "createOrganization": {
            "httpMethod": "POST",
            "extends": "abstract.basezohoorgapi",
            "uri": "/api/v3/organizations",
            "summary": "Create an organization",
            "parameters": {
                "name": {
                    "location": "json",
                    "description": "Organization name",
                    "required": true
                },
                "fiscal_year_start_month": {
                    "location": "json",
                    "type": "string",
                    "description": "Fiscal or financial starting year of your business. Allowed Values: january, february, march, april, may, june, july, august, september, october, november and december"
                },
                "currency_code": {
                    "location": "json",
                    "description": "Organization currency",
                    "required": true,
                    "default": "EUR"
                },
                "time_zone": {
                    "location": "json",
                    "description": "Organization time zone",
                    "required": true,
                    "default": "Europe/Paris"
                },
                "date_format": {
                    "location": "json",
                    "description": "Date format",
                    "default": "dd/MM/yyyy"
                },
                "field_separator": {
                    "location": "json",
                    "description": "Date field separator"
                },
                "language_code": {
                    "location": "json",
                    "description": "Language",
                    "default": "en"
                },
                "tax_basis": {
                    "location": "json",
                    "description": "Tax basis",
                    "default": "en"
                },
                "tax_type": {
                    "location": "json",
                    "description": "Tax type",
                    "default": "en"
                },
                "address": {
                    "location": "json",
                    "description": "Organization address",
                    "type": "object",
                    "parameters": {
                        "street_address1": {
                            "location": "json",
                            "type": "string"
                        },
                        "street_address2": {
                            "location": "json",
                            "type": "string"
                        },
                        "city": {
                            "location": "json",
                            "type": "string"
                        },
                        "state": {
                            "location": "json",
                            "type": "string"
                        },
                        "zip": {
                            "location": "json",
                            "type": "string"
                        },
                        "country": {
                            "location": "json",
                            "type": "string"
                        }
                    }
                }
            }
        },
        "updateOrganization": {
            "httpMethod": "PUT",
            "extends": "abstract.basezohoorgapi",
            "uri": "/api/v3/organizations",
            "summary": "Update an organization",
            "parameters": {
                "name": {
                    "location": "json",
                    "description": "Organization name"
                },
                "is_logo_uploaded": {
                    "location": "json",
                    "description": "To determine whether logo is uploaded"
                },
                "fiscal_year_start_month": {
                    "location": "json",
                    "description": "Organization name"
                },
                "currency_code": {
                    "location": "json",
                    "description": "Organization currency",
                    "default": "EUR"
                },
                "time_zone": {
                    "location": "json",
                    "description": "Organization time zone",
                    "default": "Europe/Paris"
                },
                "date_format": {
                    "location": "json",
                    "description": "Date format",
                    "default": "dd/MM/yyyy"
                },
                "field_separator": {
                    "location": "json",
                    "description": "Date field separator"
                },
                "language_code": {
                    "location": "json",
                    "description": "Language",
                    "default": "en"
                },
                "tax_basis": {
                    "location": "json",
                    "description": "Tax basis",
                    "default": "en"
                },
                "address": {
                    "location": "json",
                    "description": "Organization address",
                    "type": "object",
                    "parameters": {
                        "street_address1": {
                            "location": "json",
                            "type": "string"
                        },
                        "street_address2": {
                            "location": "json",
                            "type": "string"
                        },
                        "city": {
                            "location": "json",
                            "type": "string"
                        },
                        "state": {
                            "location": "json",
                            "type": "string"
                        },
                        "zip": {
                            "location": "json",
                            "type": "string"
                        },
                        "country": {
                            "location": "json",
                            "type": "string"
                        }
                    }
                }
            }
        }
    }

}
